{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.customDictionaries": {},
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "package.json",
    "services.yaml",
    "composer.json"
  ],
  "cSpell.ignoreRegExpList": ["/import\\s+[^;]*;/", "/.css-[a-zA-Z0-9-]+/"],
  "cSpell.language": "fr,en",
  "cSpell.languageSettings": [
    {
      "caseSensitive": false,
      "languageId": "*",
      "locale": "fr"
    }
  ],
  "cSpell.words": [],
  "prettier.tabWidth": 2,
  "prettier.useTabs": false
}
